source `dirname $0`/util.sh

mkdir $workdir/inner
RECORD_ARGS="--env=_RR_TRACE_DIR=$workdir/inner"
record $TESTNAME "$(which rr) record --nested=detach $PWD/$TESTNAME-$nonce --inner"
# Replay outer
replay
check_record EXIT-WAITED
# Replay inner
cd $workdir/inner
workdir=$PWD
replay
check_replay_token EXIT-SUCCESS
